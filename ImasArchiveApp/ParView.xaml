<UserControl
    x:Class="ImasArchiveApp.ParView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:ImasArchiveApp"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <DockPanel>
        <DockPanel DockPanel.Dock="Left">
            <ContentControl Content="{Binding Path=BrowserModel}" DockPanel.Dock="Left" />
        </DockPanel>
        <Border BorderBrush="#FFABADB3" BorderThickness="1">
            <ContentControl Content="{Binding Path=CurrentFileModel}" />
        </Border>
    </DockPanel>
</UserControl>
